super  + Return
	   st

alt + w
	$BROWSER


alt + F9
	dmenu_record


alt + F10 
		guvcview


alt + F11
	dmenu_screenshot


alt + shift + p 
		 passmenu -i -r -l 10 -p "passmenu:"

alt + shift + o
		 clipmenu -i -r -l 10 -p "clipmenu:"


alt + shift + m 
	~/src/scripts/mount.sh



alt + shift + {equal,minus,BackSpace}
	volumectl {inc,dec,toggle}



XF86MonBrightnessUp
	xbacklight -inc 10

XF86MonBrightnessDown
	xbacklight -dec 10



alt + r 
	dmenu_run


